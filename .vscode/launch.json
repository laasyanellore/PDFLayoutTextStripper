    "version": "0.2.0",
    "configurations": [

        {
            "type": "java",
            "request": "attach",
            "name": "Attach by Process ID",
            "processId": "${command:PickJavaProcess}"
        },
        {
            "type": "java",
            "name": "test",
            "request": "launch",
            "mainClass": "test",
            "projectName": "PDFLayoutTextStripper"
        },
        {
            "type": "java",
            "name": "Attach",
            "request": "attach",
            "hostName": "localhost",
            "port": "<debug port of the debuggee>"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        }
    ],
    "java.configuration.runtimes": [
        {
          "name": "JavaSE-1.8",
          "path": "/path/to/jdk-8",
        },
        {
          "name": "JavaSE-11",
          "path": "/path/to/jdk-11",
        },
        {
          "name": "JavaSE-19",
          "path": "/path/to/jdk-19",
          "default": true
        },
      ]
}